<div class="container-fluid">
  <p id="notice"><%= notice %></p>

  <h1>Pharmacies</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Ibge</th>
        <th>Uf</th>
        <th>Cidade</th>
        <th>Nome</th>
        <th>Endereco</th>
        <th>Bairro</th>
        <th>Cep</th>
        <th>Ddd</th>
        <th>Cnpj farmacia</th>
        <th>Cnpj matriz</th>
        <th>Ano</th>
        <th>Mes</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @pharmacies.each do |pharmacy| %>
        <tr>
          <td><%= pharmacy.ibge %></td>
          <td><%= pharmacy.uf %></td>
          <td><%= pharmacy.cidade %></td>
          <td><%= pharmacy.nome %></td>
          <td><%= pharmacy.endereco %></td>
          <td><%= pharmacy.bairro %></td>
          <td><%= pharmacy.cep %></td>
          <td><%= pharmacy.cnpj_farmacia %></td>
          <td><%= pharmacy.cnpj_matriz %></td>
          <td><%= pharmacy.ano %></td>
          <td><%= pharmacy.mes %></td>
          <td><%= link_to 'Show', pharmacy %></td>
          <td><%= link_to 'Edit', edit_pharmacy_path(pharmacy) %></td>
          <td><%= link_to 'Destroy', pharmacy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Pharmacy', new_pharmacy_path %>
  <br>
    <%= paginate(@pharmacies) %>
  <br>
</div>
